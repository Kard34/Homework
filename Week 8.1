#include <stdio.h>
#include <limits.h>

int main()
{
    int min = INT_MAX, max = INT_MIN;
    for (int i = 0; i < 3; i++)
    {
        int num;
        scanf("%d", &num);
        if (num > max)
            max = num;
        if (num < min)
            min = num;
    }
    printf("%d", min + max);
    return 0;
}
