#include <stdio.h>
#include <ctype.h>

char Ato9[] = "ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890";
char *morsecode[] = {".-", "-...", "-.-.", "-..", ".", "..-.", "--.", "....", "..", ".---", "-.-", ".-..", "--", "-.", "---", ".--.", "--.-", ".-.", "...", "-", "..-", "...-", ".--", "-..-", "-.--", "--..", ".----", "..---", "...--", "....-", ".....", "-....", "--...", "---..", "----.", "-----"};

void translate(char alphabet)
{
    for (int i = 0; i < 26 + 10; i++)
        if (alphabet == Ato9[i])
            printf("%s ", morsecode[i]);
}

int main()
{
    char txt[100];
    printf("Input (A-Z, 0-9):");
    scanf("%[^\n]", txt);
    for (int i = 0; txt[i] != '\0'; i++)
    {
        txt[i] = toupper(txt[i]);
        if ((txt[i] >= 'A' && txt[i] <= 'Z') || (txt[i] >= '0' && txt[i] <= '9'))
            translate(txt[i]);
        else
            printf("? ");
    }
    return 0;
}
