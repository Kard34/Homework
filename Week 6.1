#include <stdio.h>

int num[3];

void swap(int x, int y)
{
    int temp = num[x];
    num[x] = num[y];
    num[y] = temp;
}

int main()
{
    char com[4];
    for (int i = 0; i < 3; i++)
        scanf("%d", &num[i]);
    scanf("%s", com);
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 2; j++)
            if (num[j] > num[j + 1])
                swap(j, j + 1);
    for (int i = 0; i < 3; i++)
    {
        if (com[i] == 'A')
            printf("%d ", num[0]);
        else if (com[i] == 'B')
            printf("%d ", num[1]);
        else if (com[i] == 'C')
            printf("%d ", num[2]);
    }
    return 0;
}
