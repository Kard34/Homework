#include <stdio.h>

int main()
{
    int num;
    scanf("%d", &num);
    printf("%d", num);
    int sum, di;
    while (1)
    {
        sum = 0;
        di = 1;
        while (1)
        {
            if (num / di == 0)
                break;
            di *= 10;
        }
        di /= 10;
        if (di == 1)
            break;
        for (int i = di; i > 0; i /= 10)
        {
            int tmp = num / i;
            sum += tmp;
            num -= i * tmp;
        }
        num = sum;
        printf(" -> %d", sum);
    }
    return 0;
}
