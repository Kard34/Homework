#include <stdio.h>
#include <string.h>

char *alphabet[] = {".-", "-...", "-.-.", "-..", ".", "..-.", "--.", "....", "..", ".---", "-.-", ".-..", "--", "-.", "---", ".--.", "--.-", ".-.", "...", "-", "..-", "...-", ".--", "-..-", "-.--", "--.."};
char *num[] = {".----", "..---", "...--", "....-", ".....", "-....", "--...", "---..", "----.", "-----"};
//A-Z, 0-9
char translate(char txt[])
{
    for (int i = 0; i < 26; i++)
        if (strcmp(txt, alphabet[i]) == 0)
            return 'A' + i;
    for (int i = 0; i < 10; i++)
        if (strcmp(txt, num[i]) == 0)
            return '0' + i;
}

int main()
{
    char txt[100];
    scanf("%[^\n]", txt);
    int t = 0;
    char tmp[100];
    for (int i = 0; txt[i] != '\0'; i++)
    {
        if (txt[i] != ' ')
        {
            tmp[t] = txt[i];
            t++;
        }
        else
        {
            tmp[t] = '\0';
            t = 0;
            printf("%c ", translate(tmp));
        }
    }
    tmp[t] = '\0';
    printf("%c", translate(tmp));
    return 0;
}
