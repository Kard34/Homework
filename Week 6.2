#include <stdio.h>
#include <limits.h>

int main()
{
    int num[3];
    char com[4];
    int min = INT_MAX, mid, max = INT_MIN;
    for (int i = 0; i < 3; i++)
    {
        scanf("%d", &num[i]);
        if (min > num[i])
            min = num[i];
        if (max < num[i])
            max = num[i];
    }
    scanf("%s", com);
    for (int i = 0; i < 3; i++)
        if (num[i] != min && num[i] != max)
            mid = num[i];
    for (int i = 0; i < 3; i++)
    {
        if (com[i] == 'A')
            printf("%d ", min);
        else if (com[i] == 'B')
            printf("%d ", mid);
        else if (com[i] == 'C')
            printf("%d ", max);
    }
    return 0;
}
