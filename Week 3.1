#include <stdio.h>
#include <limits.h>

int main()
{
    int arr[3], min = INT_MAX, mid = INT_MAX;
    for (int i = 0; i < 3; i++)
    {
        scanf("%d", &arr[i]);
        if (arr[i] < min)
            min = arr[i];
    }
    for (int i = 0; i < 3; i++)
    {
        if (arr[i] < mid && arr[i] != min)
            mid = arr[i];
    }
    printf("%d", min + mid);
    return 0;
}
