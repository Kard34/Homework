#include <stdio.h>
#include <string.h>

int main()
{
    char text[100000];
    scanf("%[^\n]", text);
    int n = strlen(text);
    for (int i = 0; i < n / 2; i++)
    {
        char temp = text[i];
        text[i] = text[n - i - 1];
        text[n - i - 1] = temp;
    }
    printf("%s", text);
    return 0;
}
