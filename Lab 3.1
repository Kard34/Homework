#include <stdio.h>

int main()
{
    int num, result;
    scanf("%d", &num);
    printf("%d", num);
    while (num > 9)
    {
        result = 0;
        while (num != 0)
        {
            result += num % 10;
            num /= 10;
        }
        num = result;
        printf(" -> %d", result);
    }
    return 0;
}
