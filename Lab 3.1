#include <stdio.h>

int main()
{
    int num, result;
    scanf("%d", &num);
    printf("%d", num);
    while (1)
    {
        result = 0;
        while (num != 0)
        {
            result += num % 10;
            num /= 10;
        }
        num = result;
        printf(" -> %d", result);
        if (result < 10)
            break;
    }
    return 0;
}
