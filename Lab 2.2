#include <iostream>

using namespace std;

int main()
{
    int x, y, z;
    cout << "Enter first number : ";
    cin >> x;
    cout << "Enter second number : ";
    cin >> y;
    cout << "Greatest common divisor = ";
    while (1)
    {
        z = x % y;
        if (z == 0)
        {
            cout << y;
            break;
        }
        x = y;
        y = z;
    }
    return 0;
}
