#include <stdio.h>

int main()
{
    int n, x, y;
    scanf("%d", &n);
    int sum[n + 1];
    sum[0] = 0;
    for (int i = 0; i < n; i++)
    {
        int num;
        scanf("%d", &num);
        sum[i + 1] = sum[i] + num;
    }
    scanf("%d", &x);
    scanf("%d", &y);
    printf("%d", sum[y] - sum[x - 1]);
    return 0;
}
